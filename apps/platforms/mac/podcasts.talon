os:mac
app: podcasts
-

(play|pause):
  key(space)

new station:
  key(cmd-n)

(reload|refresh):
  key(cmd-r)

hunt [<user.text>]:
  key(cmd-f)
  insert(user.text)

go to show:
  key(cmd-l)

skip forward:
  key(shift-cmd-right)

rewind:
  key(shift-cmd-right)

normal speed:
  key(alt-1)

faster speed:
  key(alt-3)

double speed:
  key(alt-4)
