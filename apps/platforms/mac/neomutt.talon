os: mac
app: iterm2
win.title: /neomutt/
-

archive:     key(a)
cancel:      key(ctrl-c n)
compose:     key(m)
confirm:     key(enter y)
delete:      key(d)
(exit|quit): key(q)
open:        key(enter)
reload:      key(shift-o)
tag:         key(t)
archive all: key(; a)
delete all:  key(; d)
reply:       key(r)
save:        key(shift-s)
send:        key(y)

(group reply|reply all): key(g)

next: key(j)
(back|previous): key(k)
